{
  "name": "bookstore-backend",
  "version": "0.0.1",
  "description": "Bookstore application backend with NestJS",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check-modules-zones": "ts-node ./scripts/check-existence-modules-eslint-zones.ts",
    "lint:update-zones": "ts-node ./scripts/update-eslint-zones.ts",
    "test": "vitest run",
    "test:e2e": "vitest run src/modules/book/boundary/book.controller.e2e-spec.ts",
    "test:types": "tsc --noEmit --project .",
    "migrate": "knex migrate:latest",
    "migrate:make": "read -p 'Enter migration name: ' migrationName && knex migrate:make $migrationName && ts-node ./scripts/create-migration-sql-file.ts $migrationName"
  },
  "dependencies": {
