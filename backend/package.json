    "fastify": "^4.29.1",
    "jsdom": "^26.1.0",
    "knex": "^3.1.0",
    "nestjs-cls": "^6.0.1",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.3",
    "pino-http": "^10.5.0",
